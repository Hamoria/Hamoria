{
	"name": "@forge42/base-stack",
	"version": "0.0.1",
	"author": "forge-42",
	"private": true,
	"sideEffects": false,
	"license": "MIT",
	"type": "module",
	"scripts": {
		"execute": "tsx",
		"clean": "git clean -fdX --exclude=\"!.env\"",
		"script": "tsx scripts/setup.ts",
		"cleanup": "pnpm run script scripts/cleanup.ts",
		"build-base": "react-router build",
		"dev": "react-router dev",
		"start-base": "NODE_ENV=production node ./build/server/index.js",
		"build": "react-router build && vite -c vite.mikro-orm.config.ts build",
		"start": "NODE_ENV=production node ./build/server/index.js",
		"test": "vitest run --browser.headless",
		"test:ui": "vitest",
		"test:cov": "vitest run --coverage --browser.headless",
		"typecheck": "tsc",
		"validate": "pnpm run check && pnpm run typecheck && pnpm run test && pnpm run check:unused",
		"check": "biome check .",
		"check:fix": "biome check --fix .",
		"check:unused": "knip --max-issues 1",
		"check:unused:fix": "knip --fix",
		"typegen": "react-router typegen",
		"postinstall": "pnpm run typegen",
		"preinstall": "npx --yes only-allow pnpm",
		"mikro-orm": "mikro-orm",
		"schema:create": "mikro-orm schema:create -r",
		"schema:update": "mikro-orm schema:update -r",
		"migration:create": "mikro-orm migration:create",
		"migration:up": "mikro-orm migration:up",
		"migration:down": "mikro-orm migration:down"
	},
	"mikro-orm": {
		"configPaths": [
			"./app/server/lib/db/configs/dev.ts",
			"./build/db/mikro-orm.config.js"
		]
	},
	"dependencies": {
		"@acab/reset.css": "0.11.0",
		"@babel/plugin-proposal-class-properties": "7.18.6",
		"@babel/plugin-transform-class-properties": "7.25.9",
		"@conform-to/react": "1.12.0",
		"@conform-to/zod": "1.12.0",
		"@edgefirst-dev/api-client": "0.3.0",
		"@epic-web/client-hints": "1.3.8",
		"@forge42/seo-tools": "1.4.5",
		"@mikro-orm/core": "6.5.7",
		"@mikro-orm/migrations": "6.5.7",
		"@mikro-orm/mongodb": "6.5.7",
		"@mikro-orm/reflection": "6.5.7",
		"@node-rs/argon2": "2.0.2",
		"@node-rs/argon2-wasm32-wasi": "2.0.2",
		"@radix-ui/react-avatar": "1.1.10",
